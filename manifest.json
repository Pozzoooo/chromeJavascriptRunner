{
  "name": "Javascript Automator",
  "version": "1.0",
  "description": "Javscript magic button",
  "permissions": ["storage", "declarativeContent", "activeTab", "tabs"],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": {
	"default_icon": "images/get_started16.png"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "commands": {
    "playPause": {
      "suggested_key": {
        "default": "Ctrl+Down"
      },
      "description": "Play/Pause"
    }
  },
  "manifest_version": 2
}

